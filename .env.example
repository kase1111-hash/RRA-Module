# Example environment configuration for RRA Module
# Copy this to .env and fill in your values

# Ethereum Configuration
ETHEREUM_NETWORK=mainnet
ETHEREUM_PROVIDER_URL=https://mainnet.infura.io/v3/YOUR_INFURA_KEY
ETHEREUM_WALLET_ADDRESS=0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb
ETHEREUM_PRIVATE_KEY=your_private_key_here

# Contract Addresses (after deployment)
REPO_LICENSE_CONTRACT_ADDRESS=

# GitHub Configuration (for API access)
GITHUB_TOKEN=your_github_token_here

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_SECRET_KEY=your_secret_key_here

# Storage Paths
WORKSPACE_DIR=./cloned_repos
KNOWLEDGE_BASE_DIR=./agent_knowledge_bases
REPUTATION_DATA_DIR=./reputation_data

# Logging
LOG_LEVEL=INFO
LOG_FILE=rra.log
